{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "server.js",
        "methods": ["GET", "POST"]
      },
      {
        "src": "/(.*)",
        "dest": "public/$1"
      }
    ]
  }